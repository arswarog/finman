  "scripts": {
    "start": "react-scripts start",
    "install": "echo \"#!/bin/sh\nnpm run commitlint\" > .git/hooks/commit-msg",
    "postinstall": "chmod 755 .git/hooks/commit-msg",
    "commitlint": "commitlint -e .git/COMMIT_EDITMSG",
    "prebuild": "npm run sw:clear",
    "build": "react-scripts build",
    "postbuild": "npm run sw:build",
    "test": "react-scripts test --coverage",
    "coverage": "CI=true react-scripts test --coverage",
    "eject": "react-scripts eject",
    "serve": "serve",
    "sw:clear": "rm -f publish/service-worker.js",
    "sw:serve": "webpack --config ./webpack.sw.dev.config.js --progress --colors",
    "sw:build": "webpack --config ./webpack.sw.prod.config.js --progress --colors",
    "predeploy": "npm run build",
    "deploy": "s3cmd --storage-class STANDARD put build/* s3://www.4wallet.app -r",
    "docs": "typedoc",
    "predocs:deploy": "npm run docs",
    "docs:deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://4wallet.app/"
